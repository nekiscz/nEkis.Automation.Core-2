<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="CoreSettings" type="nEkis.Automation.Core.Environment.Configuration.CoreSection, nEkis.Automation.Core" />
    <section name="LogSettings" type="nEkis.Automation.Core.Environment.Configuration.LogSection, nEkis.Automation.Core" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <CoreSettings>
    <TestSettings TestSetting="Url" Value="http://google.com" />
    <TestSettings TestSetting="DefaultBrowser" Value="chrome" />
  </CoreSettings>

  <LogSettings>
    <DateSettings>
      <add DateFormat="DateFormat" Format="yyyyMMdd" />
      <add DateFormat="DateTimeFormat" Format="yyyyMMdd-HHmmss" />
      <add DateFormat="ReadableDateFormat" Format="yyyy. MM. dd." />
      <add DateFormat="ReadableDateTimeFormat" Format="yyyy. MM. dd. - HH:mm:ss" />
    </DateSettings>
    <PathSettings>
      <add File="Screenshots" Path="/screenshots" />
    </PathSettings>
  </LogSettings>
</configuration>